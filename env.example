# Facebook Scraper Configuration
# Скопируйте этот файл в .env и заполните нужные значения

# Путь к файлу с cookies для Facebook
FACEBOOK_COOKIES_FILE=facebook/cookies.txt

# Канал браузера для использования Playwright
# Доступные варианты: chrome, chrome-beta, msedge, msedge-beta, msedge-dev
FACEBOOK_BROWSER_CHANNEL=chrome

# Или путь к исполняемому файлу браузера (если не используете канал)
# FACEBOOK_BROWSER_PATH=/Applications/Google Chrome.app/Contents/MacOS/Google Chrome

# HuggingFace Configuration
HF_API_KEY=your_huggingface_api_key_here
HF_DEFAULT_MODEL=Qwen/Qwen2.5-7B-Instruct:together

# ChromaDB Configuration
CHROMA_HOST=localhost
CHROMA_PORT=8000
CHROMA_COLLECTION_NAME=gaado_documents
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# ChromaDB API (если используете облачный сервис)
# CHROMA_API_URL=https://your-chromadb-instance.com
# CHROMA_API_KEY=your_chromadb_api_key

